            Dim xlSh As Excel.Worksheet = Nothing
            Dim rg As Excel.Range = Nothing


            xlObject = New Excel.Application()
            xlObject.AlertBeforeOverwriting = False
            xlObject.DisplayAlerts = False

            xlWB = xlObject.Workbooks.Add()
            xlWB.SaveAs(path)
            xlSh = DirectCast(xlObject.ActiveWorkbook.ActiveSheet, Excel.Worksheet)

            Dim dc As System.Data.DataColumn
            Dim dr As System.Data.DataRow
            Dim colIndex As Integer = 0
            Dim rowIndex As Integer = 0
            For Each dc In dt.Columns
                colIndex = colIndex + 1
                xlSh.Cells(1, colIndex) = dc.ColumnName
            Next
            For Each dr In dt.Rows
                rowIndex = rowIndex + 1
                colIndex = 0
                For Each dc In dt.Columns
                    colIndex = colIndex + 1
                    xlSh.Cells(rowIndex + 1, colIndex) = dr(dc.ColumnName)
                Next
            Next
            xlSh.Columns.AutoFit()
            xlWB.SaveAs(path)

            ReleaseObject(xlSh)
            xlWB.Close(False)
            ReleaseObject(xlWB)
            xlObject.Quit()
            ReleaseObject(xlObject)
            GC.Collect()

