          SQLTask:SqlStatementSource="Select s.ID&#xA;&#x9;,isnull(File_path,'') as File_path&#xA;&#x9;,isnull(File_Name,'') as File_Name&#xA;&#x9;,isnull(Column_delimiter,'') as Column_delimiter&#xA;&#x9;, isnull(Sheet_name,'') as Sheet_name&#xA;&#x9;,isnull(MonthYearfolder,'') as MonthYearfolder&#xA;&#x9;,isnull(data_source_query,'') as data_source_query&#xA;&#x9;,isnull(s.column_names,'') as  Column_names&#xA;&#x9;--,isnull(File_extension,'') as File_extension&#xA;&#x9;,isnull(sub_folder,'') as sub_folder&#xA;&#x9;,CASE WHEN MonthYearfolder = 1  and isnull(ss.schedule_month,0) = 0 then convert(varchar(4),YEAR(getdate())) + '\' + RIGHT('00' + convert(varchar(2),month(getdate())),2) + '\'&#xA;&#x9;  WHEN MonthYearfolder = 1  and isnull(ss.schedule_month,0) &gt; 0 then convert(varchar(4),YEAR(getdate())) + '\' + RIGHT('00' + convert(varchar(2),ss.schedule_month),2) + '\'&#xA;&#x9;  else '' END as Yearmonthfolder&#xA;&#x9;,isnull([Header_rows_skip_flag],'') as [Header_rows_skip_flag]&#xA;&#x9;--,Schedule_month,Schedule_Day,schedule_month_all_flag&#xA;&#x9;,isnull(e.[From],'') as [From],isnull([TO],'') as [TO],isnull([CC],'') as [CC],isnull([BCC],'') as [BCC],isnull([Subject],'') as [Subject]&#xA;&#x9;,isnull(e.body,'') as body&#xA;&#x9;,isnull(s.savefile_flag ,0) as savefile_flag&#xA;from [dbo].[cfg_subscription] s &#xA;left join [DataWarehouseDW].[dbo].[cfg_subscription_email] e on e.ID = s.ID&#xA;left join [dbo].[cfg_subscription_schedule] ss&#xA;on s.id = ss.id where isnull(Schedule_end_date,'2999-12-31 00:00:00') &gt;=getdate() and schedule_start_date &lt;= getdate()&#xA;--and ((ss.schedule = 'Monthly' and  Schedule_Day = day(getdate()) and (Schedule_month = month(getdate()) OR isnull(schedule_month_all_flag,'FALSE') = 'TRUE' ))&#xA;and ((ss.schedule = 'Monthly' and isnull(schedule_month_all_flag,'FALSE') = 'TRUE' AND Schedule_Day = day(getdate()))&#xA;  OR (ss.schedule = 'Monthly'and isnull(schedule_month_all_flag,'FALSE') = 'FALSE' and Schedule_Day = day(getdate()) AND Schedule_month = MONTH(getdate()))&#xA;OR ss.schedule = 'Daily' &#xA;OR (ss.schedule = 'Weekly' and Schedule_weekday = datename(weekday,getdate())))&#xA;ORDER By s.ID"
