        'Works perfectly starts from here
        Dim str As String = vbNullString
        Dim path As String = Dts.Variables("User::File_path").Value.ToString() + Dts.Variables("User::File_name").Value.ToString()
        Dim delimiter_str As String = UCase(Dts.Variables("Column_delimiter").Value.ToString)
        Dim sb As New Text.StringBuilder
        Dim Hdr_row As String = Dts.Variables("column_names").Value.ToString
        Dim Hdr_row_skip_flag As String = Dts.Variables("Header_rows_skip_flag").Value.ToString()

        Dim myADONETConnection As SqlClient.SqlConnection = New SqlConnection()
        myADONETConnection = DirectCast(Dts.Connections("ADO_DataWarehouseDW").AcquireConnection(Dts.Transaction), SqlClient.SqlConnection)
        Dim queryString As String = "Select * from dbo.subscriptionsdata"
        'Dts.Variables("User::Data_source_query").Value.ToString()
        Dim x As SqlDataAdapter = New SqlDataAdapter(queryString, myADONETConnection)
        Dim ds As DataSet = New DataSet()
        x.Fill(ds)
        Dim dt As DataTable = ds.Tables(0)


        If Dts.Variables("Is_Excel").Value.ToString() = "True" Then
            ''new try starts here 
